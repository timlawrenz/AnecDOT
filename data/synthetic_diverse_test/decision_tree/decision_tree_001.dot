digraph LoanApproval {
    rankdir=TB;
    node [shape=box];

    // Root Node
    0 [label="Credit Score >= 700?"];

    // Level 1 Nodes
    1 [label="Debt-to-Income Ratio <= 36%?"];
    2 [label="Loan Amount <= $50,000?"];
    3 [label="Employment Length >= 2 years?"];

    // Level 2 Nodes
    4 [label="Income >= $60,000?"];
    5 [label="Homeownership: Own?"];
    6 [label="Purpose: Debt Consolidation?"];
    7 [label="Credit History Length >= 5 years?"];

    // Level 3 Nodes
    8 [label="Liquid Assets > $10,000?"];
    9 [label="Previous Delinquencies > 0?"];
    10 [label="Loan Term <= 36 months?"];
    11 [label="Recent Credit Inquiries <= 1?"];

    // Leaf Nodes (Decisions)
    12 [label="Approved", shape=ellipse];
    13 [label="Denied", shape=ellipse];
    14 [label="Review", shape=ellipse];
    15 [label="Conditional Approval", shape=ellipse];


    // Edges
    0 -> 1 [label="Yes"];
    0 -> 2 [label="No"];

    1 -> 4 [label="Yes"];
    1 -> 3 [label="No"];

    2 -> 5 [label="Yes"];
    2 -> 6 [label="No"];

    4 -> 12 [label="Yes"];
    4 -> 14 [label="No"];

    3 -> 12 [label="Yes"];
    3 -> 13 [label="No"];

    5 -> 12 [label="Yes"];
    5 -> 13 [label="No"];

    6 -> 15 [label="Yes"];
    6 -> 13 [label="No"];

    //Adding level 3 nodes for some branches to meet depth requirement
    4 -> 8 [label="No"];
    8 -> 12 [label="Yes"];
    8 -> 14 [label="No"];

    5 -> 7 [label="No"];
    7 -> 13 [label="Yes"];
    7 -> 14 [label="No"];
    
    6 -> 9 [label="Yes"];
    9 -> 13 [label="Yes"];
    9 -> 14 [label="No"];

}